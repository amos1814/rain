{
  "parser": "babel-eslint",
  "extends": ["airbnb/base"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "no-underscore-dangle": ["error", { "allow": ["_shouldReconnect", "_connect"] } ]
  }
}
